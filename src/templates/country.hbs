{{#each this}}
<div class="card">
    <div class="card-body">
        <h1>{{name}}</h1>
        <h3 class="card-title">Capital:</h3><span class="cart-text">{{capital}}</span>
        <h3 class="card-title"> Population:</h3><span class="cart-text"> {{population}}</span>
        <ul class="languages-group">
            <h3 class="cart-title">Languages:</h3>
            {{#each languages}}
            <li class="cart-text">{{name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="card-img-top">
        <img src='{{flag}}' alt='{{name}}'>
    </div>
</div>
{{/each}}